<!DOCTYPE mapper>

<mapper namespace="com.lani.dao.MapDataDao">

	<select id ="selectListData" resultType = "com.lani.response.ResponseMapData">
		select date, location, title from date_data
	</select>

	<insert id = "insertMapData">
		insert into date_data
		values (
			"#{date}","#{location}","#{title}"
		)
	</insert>

</mapper>